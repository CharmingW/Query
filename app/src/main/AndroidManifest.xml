<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.charmingwong.query">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_query"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.IpActivity" />
        <activity android:name=".activity.DomainQueryActivity" />
        <activity android:name=".activity.IdActivity" />
        <activity android:name=".activity.PhoneActivity" />
        <activity android:name=".activity.RobotActivity" />
        <activity android:name=".activity.WordActivity"></activity>
    </application>

</manifest>